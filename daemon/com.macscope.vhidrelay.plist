<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.macscope.vhidrelay</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/libexec/macscope-vhid-relay</string>
        <string>--uid</string>
        <string>__CONSOLE_UID__</string>
    </array>
    <key>RunAtLoad</key><true/>
    <key>KeepAlive</key><true/>
    <key>StandardOutPath</key><string>/var/log/macscope-relay.stdout.log</string>
    <key>StandardErrorPath</key><string>/var/log/macscope-relay.stderr.log</string>
</dict>
</plist>
